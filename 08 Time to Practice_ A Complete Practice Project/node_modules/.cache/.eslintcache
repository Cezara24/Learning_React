arget.value = event.target.value.slice(1);\n    }\n    setEnteredAge(event.target.value);\n  };\n\n  const errorHandler = () => {\n    setError(null);\n  }\n\n  return (\n    <div>\n      {error && <ErrorModal title={error.title} message={error.message} onConfirm={errorHandler}></ErrorModal>}\n      <Card className={classes.input}>\n        <form onSubmit={addUserHandler}>\n          <label htmlFor=\"username\">Username</label>\n          <input\n            id=\"username\"\n            type=\"text\"\n            value={enteredUsername}\n            onChange={usernameChangeHandler}\n          />\n          <label htmlFor=\"age\">Age (Years)</label>\n          <input\n            id=\"age\"\n            type=\"number\"\n            min=\"0\"\n            max=\"130\"\n            value={enteredAge}\n            onChange={ageChangeHandler}\n          />\n          <Button type=\"submit\">Add User</Button>\n        </form>\n      </Card>\n    </div>\n  );\n};\n\nexport default AddUser;","/home/cezara/Documents/Learning_React/08 Time to Practice_ A Complete Practice Project/src/components/UI/Button.js",[],"/home/cezara/Documents/Learning_React/08 Time to Practice_ A Complete Practice Project/src/components/UI/Card.js",[],"/home/cezara/Documents/Learning_React/08 Time to Practice_ A Complete Practice Project/src/components/Users/UsersList.js",[],"/home/cezara/Documents/Learning_React/08 Time to Practice_ A Complete Practice Project/src/components/UI/ErrorModal.js",[],{"ruleId":"33","severity":1,"message":"34","line":43,"column":56,"nodeType":"35","messageId":"36","endLine":43,"endColumn":57,"suggestions":"37"},{"ruleId":"33","severity":1,"message":"38","line":43,"column":58,"nodeType":"35","messageId":"36","endLine":43,"endColumn":59,"suggestions":"39"},"no-useless-escape","Unnecessary escape character: \\+.","Literal","unnecessaryEscape",["40","41"],"Unnecessary escape character: \\-.",["42","43"],{"messageId":"44","fix":"45","desc":"46"},{"messageId":"47","fix":"48","de